{
  "extends": ["tslint:latest", "tslint-react"],
  "rulesDirectory": "node_modules/tslint-eslint-rules/dist/rules",
  "rules": {
      // enable tslint-react rules here 
      "jsx-no-lambda": true,
      "ter-indent": [
    true,
    2,
    {
      "FunctionExpression": {
        "parameters": 1,
        "body": 1
      }
    }
  ]}
}